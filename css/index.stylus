#map 
  height 480px
  width 100%
  display block

html, body 
  height 100%
  margin 0
  padding 0
  background-color #2EA3DD

#soapyMarker
  font-family Lato, Arial, Tahoma, sans-serif

#soapyTitle
  line-height 20px
  font-size 14px
  font-weight bold
  color #000000

#soapyAddress
  line-height 0px
  font-size 14px
  color #000000

#soapyCity
  line-height 0px
  font-size 14px
  color #000000

#soapyLink
  color #2EA3DD
  font-size 14px
  font-weight bold
  line-height 0px
  text-decoration underline

#soapyArrow
  color #2EA3DD
  url "https://dahlproperties.com/wp-content/uploads/2017/07/icon_tan_arrow_right.png"